// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main()
{
    int cnt=0;
    string str;
    
    cin>>str;
    
    for(int i=0;i<str.length();i++)
    {
        for(int j=i+1;j<str.length();j++)
        {
            if(str[i] == str[j])
            {
                cnt++;
            }
        }
    }
    cout<<endl<<cnt;
}
