
#include <iostream>
#include<string.h>
using namespace std;
int main() {
    string str1 = "Piyu";
    string str2 = "Piyushka";
    int cnt=0;
    for(int i=0;i<str1.length();i++)
    {
        if(str1[i] == str2[i])
        {
            cnt+=1;
        }
    }
    if(cnt == str1.length() && cnt== str2.length())
    {
        cout<<"Isomorphic!";
        // break;
    }
    else if(cnt != str1.length() ||  cnt!=str2.length())
    {
        cout<<"Not!";
        // break;
    }
    
    return 0;
}
