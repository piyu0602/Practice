// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

void slide_window(int size,int n,int arr[], int start)
{
    int a[size];
    int max=0;
   for (int z=0; z < n-size+1; z++)
   {
    for(int j=0;j< n-size+1;j++)
    {
        a[j] = arr[start];
        start++;
    }
        for(int i=0;i<size;i++)
        {
            if(max<a[i])
            {
                max = a[i];
            }
        }
        cout<<max<<endl;
       
    } 
    
};

int main() {
    int n , size;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    cin>>size;
    
    slide_window(size,n,arr,0) ; 
    return 0;
}
